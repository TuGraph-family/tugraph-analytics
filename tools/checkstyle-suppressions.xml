<suppressions>
    <!-- Suppresses files in the src/test folder. To be used for ruleTests which only apply to
         production code. -->
    <!--<suppress files="[\/]src[\\/]\test[\\/].*" id="TestScope"/>-->
    <suppress checks=".*" files=".*Test.java"/>
    <suppress checks=".*" files="target[\\/].*"/>
    <suppress checks=".*" files="PushDownPb.java"/>
    <suppress checks=".*" files="ViewMetaPb"/>
    <suppress checks="MethodName" files="GremlinGraph"/>
    <suppress checks="MethodName" files="GraphState"/>
    <suppress checks="AvoidEscapedUnicodeCharacters|IllegalTokenText" files="StateConfigKeys.java"/>

</suppressions>
