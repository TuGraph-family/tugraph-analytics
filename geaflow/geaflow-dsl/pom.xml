                <groupId>com.antgroup.tugraph</groupId>
                <artifactId>geaflow-dsl-connector-console</artifactId>
                <version>${project.version}</version>
            </dependency>

            <dependency>
                <groupId>com.antgroup.tugraph</groupId>
                <artifactId>geaflow-dsl-connector-file</artifactId>
